<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.girbola.controllers.datefixer.AskEventDialogController">
   <children>
      <VBox spacing="3.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="location_lbl" text="%location" />
                  <ComboBox fx:id="location_cmb" editable="true" maxWidth="1.7976931348623157E308" promptText="Location" GridPane.columnIndex="1" />
                  <Label fx:id="event_lbl" text="%event" GridPane.rowIndex="1" />
                  <ComboBox fx:id="event_cmb" editable="true" maxWidth="1.7976931348623157E308" promptText="Event" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="event_lbl1" layoutX="10.0" layoutY="47.0" text="%user" GridPane.rowIndex="2" />
                  <ComboBox fx:id="user_cmb" editable="true" layoutX="110.0" layoutY="43.0" maxWidth="1.7976931348623157E308" promptText="User" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <HBox alignment="CENTER_LEFT" spacing="3.0">
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="3.0" VBox.vgrow="NEVER">
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_RIGHT" fillHeight="false" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="apply_btn" mnemonicParsing="false" onAction="#apply_btn_action" text="%apply" />
                  <Button fx:id="apply_and_copy_btn" mnemonicParsing="false" onAction="#apply_and_copy_btn_action" text="%applyAndCopy" />
                  <Button fx:id="cancel_btn" cancelButton="true" mnemonicParsing="false" onAction="#cancel_btn_action" text="%cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
