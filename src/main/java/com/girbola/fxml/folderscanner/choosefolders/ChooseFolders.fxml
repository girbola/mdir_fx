<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.girbola.controllers.folderscanner.choosefolders.ChooseFoldersController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="2.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TreeView fx:id="chooseFolders_treeView" VBox.vgrow="ALWAYS" />
            <HBox alignment="CENTER_RIGHT" fillHeight="false" spacing="2.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="ok" mnemonicParsing="false" onAction="#ok_action" text="%ok" />
                  <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel_action" text="%cancel" />
               </children>
               <padding>
                  <Insets bottom="1.0" right="2.0" top="1.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
